
public class Pair {
	private char letter;
	private int count;
	
	public Pair(){
		this.count = 0;
	}
	
	public Pair(char letter) {
		this.letter = letter;
		this.count = 0;
	}

	public char getLetter() {
		return letter;
	}

	public void setLetter(char letter) {
		this.letter = letter;
	}

	public int getCount() {
		return count;
	}

	public void addCount(){
		count++;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Pair other = (Pair) obj;
		if (letter != other.letter)
			return false;
		return true;
	}

	@Override
	public String toString() {
		return letter + " : " + count+"\n";
	}
	
	
	
	
}
