import java.util.*;

public class GCD {
	public static void main(String[] args) {
		Scanner kbd = new Scanner(System.in);
		int method, first, second, gcd;
		
		System.out.print("Enter 1 for Method1 or 2 for Method2:");
		method = kbd.nextInt();
		
		System.out.print("Enter the first integer:");
		first = kbd.nextInt();
		
		System.out.print("Enter the second integer:");
		second = kbd.nextInt();
		
		long startTime, endTime, executionTime;
		startTime = System.currentTimeMillis();
		if(method == 1){
			gcd = GCDMethod1(first,second);
		} else if(method == 2){
			gcd = GCDMethod2(first,second);
		}
		endTime = System.currentTimeMillis();
		executionTime = endTime - startTime;
		
		System.out.println("GCD is "+gcd);
		System.out.println("Execution Time: "+ executionTime +"ms");
	}
	
	public static int GCDMethod1(int a, int b){
		int gcd = 1;
		int min = Math.min(a, b);
		
		for(int i = 2; i < min; i++){
			if(a%i == 0 && b%i == 0){
				gcd = i;
			}
		}
		return gcd;
	}
	
	public static int GCDMethod2(int a, int b){
		int gcd = 1;
		int min = Math.min(a, b)/2;
		
		for(int i = min; i > 1; i--){
			if(a%i == 0 && b%i == 0){
				gcd = i;
				i = 0;
			}
		}
		
		return gcd;
	}
}
