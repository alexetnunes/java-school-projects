
import java.util.*;
public class CountDownDemo {

	public static void main(String[] args) {
		Scanner kbd = new Scanner(System.in);
		int n;
		System.out.print("");
		countDown(n);

	}
	
	public static void countDown(int n){
		if(n <= 0){
			System.out.print("Blast Off!");
		}else{
			System.out.print(n+" ");
			countDown(n-1);
		}
	}

}
